local invite = "https://discord.gg/wqmNjpZNJB"

if setclipboard then
    setclipboard(invite)
    if _G.DISPLAY_NOTIFICATION then
        _G.DISPLAY_NOTIFICATION("Discord The link is copied! Paste it into the browser")
    else
        print("[DISCORD] The link is copied: "..invite)
    end
else
    print("[DISCORD] The link is copied: "..invite)
end
